# comma separated
BEGIN {
    FS=","
}
{
    # age is the sixth column
    sum += $7
    # print $7
    # we need a count to divide by later
    count++
}
END {
    if (count > 0) {
        # print avg age
        print sum / count
    } else {
        # handle divide by zero
        print 0
    }
}